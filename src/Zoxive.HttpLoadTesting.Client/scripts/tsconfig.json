{
  "compileOnSave": false,
  "buidOnSave": false,
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "module": "es2015",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "lib": [
      "dom",
      "es5",
      "es2016"
    ]
  },
  "files": [
    "custom_types.d.ts",
    "app.tsx"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
